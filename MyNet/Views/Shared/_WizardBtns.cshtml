@model List<MyNet.Models.WizardModel>
@{
    Layout = null;
}

<div style="background-color:gold;width:100%;">
    @foreach (var item in Model)
    {
        if (item.isCurrent)
        {
            <span style="padding: 20px; font-style: oblique; color: #f30d0d; font-size: 50px;">@item.ProcessName</span>
        }
        else
        {
            <span style="padding :20px; font-style:oblique;font-size:30px;">@item.ProcessName</span>
        }
        if (!item.isEnd)
        {
            <span>---------</span>
        }

    }

</div>

